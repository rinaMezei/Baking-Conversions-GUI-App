<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>


<BorderPane prefHeight="177.0" prefWidth="424.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.171" fx:controller="application.BakingConversionsController">
   <bottom>
      <HBox prefHeight="100.0" prefWidth="200.0" BorderPane.alignment="CENTER">
         <children>
            <Button fx:id="btnAddRecipe" mnemonicParsing="false" onAction="#AddRecipeClicked" text="Add a Recipe" />
            <Button fx:id="btnMultiplyDivide" mnemonicParsing="false" onAction="#MultiplyDivideClicked" text="Multiply/Divide a Recipe" />
            <Button fx:id="btnSubstitute" mnemonicParsing="false" onAction="#SubstituteClicked" text="Substitute a Measurement" />
            <Button fx:id="btnExit" mnemonicParsing="false" onAction="#ExitClicked" text="Exit" />
         </children>
      </HBox>
   </bottom>
</BorderPane>
